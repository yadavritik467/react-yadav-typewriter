{
  "name": "react-yadav-typewriter",
  "version": "1.0.9",
  "description": "A lightweight typewriter effect for React",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && rollup -c --bundleConfigAsCjs"
  },
  "keywords": [
    "react",
    "typewriter",
    "animation",
    "typescript",
    "component"
  ],
  "author": "Ritik Yadav",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "rollup": "^4.21.2",
    "typescript": "^5.5.4"
  }
}
